v1 = [ 0.334364 , 0.0111148 ,0]
v2 = [ 0.333564 , -0.198405 ,       0 ];

% plot(v1(1), v1(2),'x')
% hold on
% grid on
% grid minor
% axis equal
% plot(v2(1), v2(2),'x')
% xlim([0 1.5])
% ylim([0 2.2])

dv = v2-v1

R = @(psi) [cos(psi) -sin(psi) 0; sin(psi) cos(psi) 0;0 0 1];
% psi =     atan(abs(dv(2)/dv(1)));
psi = atan2(dv(1),dv(2))
psi*180/pi
R(psi)*dv'

